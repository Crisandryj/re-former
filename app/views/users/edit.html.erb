<%= form_with model: @user, url: users_path do |form| %>
  <%= form.label "Username Here:" %>
  <%= form.text_field :username, value: "Test" %>

  <%= form.label :email %>
  <%= form.email_field :email, placeholder: "email@example.com" %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit "Update" %>
<% end %>
